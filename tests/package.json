{
  "name": "basic",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "generate": "run-p generate:*",
    "@vetster/orval": "node ../packages/orval/dist/bin/orval.js",
    "generate:cli": "yarn @vetster/orval --input ./specifications/petstore.yaml --output ./generated/cli/petstore/endpoints.ts",
    "generate:default": "yarn @vetster/orval --config ./configs/default.config.ts",
    "generate:axios": "yarn @vetster/orval --config ./configs/axios.config.ts",
    "generate:angular": "yarn @vetster/orval --config ./configs/angular.config.ts",
    "generate:react-query": "yarn @vetster/orval --config ./configs/react-query.config.ts",
    "generate:svelte-query": "yarn @vetster/orval --config ./configs/svelte-query.config.ts",
    "generate:vue-query": "yarn @vetster/orval --config ./configs/vue-query.config.ts",
    "generate:swr": "yarn @vetster/orval --config ./configs/swr.config.ts",
    "generate:multi-file": "yarn @vetster/orval --config ./configs/multi-file.config.ts",
    "generate:zod": "yarn @vetster/orval --config ./configs/zod.config.ts",
    "build": "tsc"
  },
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "@vetster/orval": "link:../packages/orval/dist",
    "typescript": "4.8.2"
  },
  "dependencies": {
    "@angular/common": "^17.1.1",
    "@angular/core": "^17.1.1",
    "@faker-js/faker": "^8.1.0",
    "@tanstack/react-query": "^4.22.0",
    "@tanstack/svelte-query": "^4.24.9",
    "@tanstack/vue-query": "^4.22.0",
    "axios": "^0.26.1",
    "msw": "^2.0.2",
    "swr": "^2.2.4",
    "vue": "^3.3.4",
    "zod": "^3.21.4"
  }
}
